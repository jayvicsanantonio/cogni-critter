{
  "name": "cogni-critter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:expo": "expo run:ios",
    "prebuild:ios": "expo prebuild -p ios --non-interactive",
    "lint": "npx @biomejs/biome check .",
    "lint:fix": "npx @biomejs/biome check --write .",
    "format": "npx @biomejs/biome format --write .",
    "start": "expo start --dev-client --host=lan --clear",
    "start:expo": "expo start --host=lan --clear",
    "dev:ios": "concurrently -n metro,ios -c cyan,magenta \"expo start --dev-client --host=lan\" \"wait-on http://localhost:8081 && expo run:ios\"",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "download-models": "node scripts/download-models.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@tensorflow-models/mobilenet": "^2.1.0",
    "@tensorflow/tfjs": "^4.13.0",
    "@tensorflow/tfjs-react-native": "1.0.0",
    "expo": "~51.0.0",
    "react": "18.2.0",
    "react-native": "0.74.7",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.16.0",
    "react-native-reanimated": "3.10.1",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.32.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@biomejs/biome": "^1.9.4",
    "@react-native/babel-preset": "0.74.89",
    "@react-native/metro-config": "0.74.89",
    "@react-native/typescript-config": "0.74.89",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "concurrently": "^8.2.2",
    "jest": "^29.6.3",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "@tensorflow/tfjs": "^4.13.0",
    "@tensorflow/tfjs-core": "^4.13.0",
    "@tensorflow/tfjs-backend-cpu": "^4.13.0",
    "@tensorflow/tfjs-backend-webgl": "^4.13.0"
  }
}
